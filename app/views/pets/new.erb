<h1>Create a new Pet</h1>
<form class="" action="/pets" method="post">
  <input type="text" name="pet_name" value="">
  <input type="text" name="owner_id" value="">
  <%@owners.each do |owner|%>
     <input type="checkbox" name="owner_id" value="<%=owner.id%>" id="<%=owner.id%>"><%=owner.name%></input>
 <%end%>
 <label>and/or, create a new owner:</label>
     <br></br>
     <label>name:</label>
       <input  type="text" name="owner_name"></input>
     <br></br>
  <input type="submit" name="Create Pet" value="Create Pet">
</form>
